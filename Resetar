function Resetar() {
  cam.play();
  fotosTiradas = 0;
  pontos = 0;
  
  aperteiRed1 = false;
  aperteiRed2 = false;
  aperteiGreen1 = false;
  aperteiGreen2 = false;
  aperteiBlue1 = false;
  aperteiBlue2 = false;
  palpiteRedX = 0;
  palpiteRedY = 0;
  palpiteGreenX = 0;
  palpiteGreenY = 0;
  palpiteBlueX = 0;
  palpiteBlueY = 0;
}
